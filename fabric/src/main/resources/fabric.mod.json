{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "KilaBash"
  ],
  "contact": {
    "homepage": "${mod_url}",
    "sources": "${mod_url}"
  },
  "license": "${mod_license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.lowdragmc.gtceu.fabric.GTCEuFabric"
    ],
    "client": [
      "com.lowdragmc.gtceu.client.fabric.ClientProxyImpl"
    ],
    "top_plugin": [
      "com.lowdragmc.gtceu.integration.top.fabric.TheOneProbePluginImpl"
    ],
    "rei_client": [
      "com.lowdragmc.gtceu.integration.rei.fabric.REIPluginImpl"
    ],
    "jei_mod_plugin": [
      "com.lowdragmc.gtceu.integration.jei.fabric.JEIPluginImpl"
    ],
    "ldlib_pugin": [
      "com.lowdragmc.gtceu.integration.ldlib.fabric.LDLibPlugin"
    ],
    "fabric-datagen": [
      "com.lowdragmc.gtceu.data.fabric.GregTechDatagen"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}-common.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "ldlib": "${ldlib_version}"
  }
}